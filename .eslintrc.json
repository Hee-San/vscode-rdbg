{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/naming-convention": ["warn", {"format": ["camelCase", "snake_case"]}],
        "@typescript-eslint/semi": "warn",
        "no-throw-literal": "warn",
        "semi": "off"
    }
}
